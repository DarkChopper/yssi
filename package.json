    "lodash": "^4.17.4",
    "materialize-css": "^0.97.8",
    "meteor-rxjs": "0.4.7",
    "ng2-pagination": "^1.0.1",
    "ngx-rating": "0.0.9",
    "reflect-metadata": "0.1.8",
